{
  "name": "mona-storybook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "compile:pug:watch": "node scripts/compile-pug-watch.mjs",
    "storybook:dev": "concurrently -k \"node scripts/compile-pug-watch.mjs\" \"storybook dev -p 6006\""
  },
  "dependencies": {
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/eslintrc": "^3",
    "@storybook/addon-a11y": "^9.1.5",
    "@storybook/addon-controls": "^9.0.8",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-vitest": "^9.1.5",
    "@storybook/nextjs-vite": "^9.1.5",
    "@types/node": "^20",
    "@types/pug": "^2.0.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "chokidar": "^4.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-plugin-storybook": "^9.1.5",
    "fast-glob": "^3.3.3",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.55.0",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.30.0",
    "pug": "^3.0.3",
    "pug-runtime": "^3.0.1",
    "sass": "^1.92.1",
    "storybook": "^9.1.5",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "concurrently": "^8.2.2"
  }
}